		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"To record detailed antimicrobial resistance patterns with standardized interpretation according to EUCAST or CLSI guidelines.">
			keywords = <"antimicrobial","resistance","susceptibility","MIC","EUCAST","CLSI","antibiogram","AST">
			use = <"Use to record resistance testing results for individual antimicrobial agents as part of antimicrobial susceptibility testing (AST). Designed for use within laboratory test archetypes.">
			misuse = <"Not to be used as standalone archetype. Must be used within laboratory test or microbiology report archetypes.">
		>
