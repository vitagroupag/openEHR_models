definition
	CLUSTER[at0000] matches {    -- Antimicrobial resistance pattern
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0001] matches {    -- Antimicrobial agent
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0002] occurrences matches {0..1} matches {    -- MIC value
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::119]>
						list = <
							["1"] = <
								units = <"mg/l">
								magnitude = <|>=0.0|>
							>
						>
					>
				}
			}
			ELEMENT[at0003] occurrences matches {0..1} matches {    -- Zone diameter
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::122]>
						list = <
							["1"] = <
								units = <"mm">
								magnitude = <|>=0.0|>
							>
						>
					>
				}
			}
			ELEMENT[at0004] matches {    -- Susceptibility result
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0005,    -- Susceptible (S)
							at0006,    -- Susceptible increased exposure (I)
							at0007,    -- Resistant (R)
							at0008,    -- Not determined
							at0009]    -- Not applicable
						}
					}
				}
			}
			ELEMENT[at0010] occurrences matches {0..1} matches {    -- Interpretation criteria
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0011,    -- EUCAST
							at0012,    -- CLSI
							at0013,    -- Local criteria
							at0014]    -- Other
						}
					}
				}
			}
			ELEMENT[at0015] occurrences matches {0..1} matches {    -- Breakpoint version
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0016] occurrences matches {0..1} matches {    -- Testing method
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0017,    -- Broth microdilution
							at0018,    -- Agar dilution
							at0019,    -- Disk diffusion
							at0020,    -- E-test
							at0021,    -- Automated system
							at0022,    -- Molecular method
							at0023]    -- Other
						}
					}
				}
			}
			ELEMENT[at0024] occurrences matches {0..1} matches {    -- MDRO classification
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0025,    -- Multidrug-resistant (MDR)
							at0026,    -- Extensively drug-resistant (XDR)
							at0027,    -- Pandrug-resistant (PDR)
							at0028,    -- ESBL producer
							at0029,    -- Carbapenemase producer
							at0030,    -- MRSA
							at0031,    -- VRE
							at0032]    -- Not classified
						}
					}
				}
			}
			CLUSTER[at0033] occurrences matches {0..*} matches {    -- Resistance mechanism
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0034] matches {    -- Mechanism type
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0035,    -- Beta-lactamase
									at0036,    -- Efflux pump
									at0037,    -- Target modification
									at0038,    -- Impermeability
									at0039,    -- Enzymatic inactivation
									at0040]    -- Other mechanism
								}
							}
						}
					}
					ELEMENT[at0041] occurrences matches {0..1} matches {    -- Specific mechanism
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0042] occurrences matches {0..1} matches {    -- Detection method
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0043,    -- Phenotypic
									at0044,    -- Genotypic
									at0045]    -- Combined
								}
							}
						}
					}
				}
			}
			ELEMENT[at0046] occurrences matches {0..1} matches {    -- Clinical significance
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0047,    -- High clinical significance
							at0048,    -- Moderate clinical significance
							at0049,    -- Low clinical significance
							at0050]    -- Unknown significance
						}
					}
				}
			}
			ELEMENT[at0051] occurrences matches {0..1} matches {    -- Epidemiological significance
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0052,    -- Alert organism
							at0053,    -- Outbreak strain
							at0054,    -- Endemic strain
							at0055]    -- Sporadic occurrence
						}
					}
				}
			}
			ELEMENT[at0056] occurrences matches {0..1} matches {    -- Quality control
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0057,    -- Within acceptable range
							at0058,    -- Outside acceptable range
							at0059]    -- Not performed
						}
					}
				}
			}
			ELEMENT[at0060] occurrences matches {0..1} matches {    -- Test date
				value matches {
					DV_DATE_TIME matches {*}
				}
			}
			ELEMENT[at0061] occurrences matches {0..1} matches {    -- Comments
				value matches {
					DV_TEXT matches {*}
				}
			}
			allow_archetype CLUSTER[at0062] occurrences matches {0..*} matches {    -- Additional details
				include
					archetype_id/value matches {/.*/}
			}
		}
	}

